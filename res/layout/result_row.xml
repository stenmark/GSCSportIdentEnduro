<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical"
	android:background="@color/white">

	<TextView		 	
		android:id="@+id/result_title"
		android:layout_width="fill_parent"
 		android:layout_height="wrap_content"
 		android:layout_marginLeft="8dp"			    
 		android:layout_marginRight="8dp"
 		android:layout_marginBottom="8dp"
 		android:textSize="24dp"
 		android:text="Namn Namnsson"	
 		android:textStyle="bold"	
 		android:textColor="@color/blue"	 		      		   
	/>	
    
    <LinearLayout 
        android:id="@+id/result_header"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:layout_marginLeft="4dp"
		android:layout_marginRight="4dp"		
		android:background="@android:color/black"
		>
		 			   
		<TextView		 	
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:textSize="16dp"		    
		    android:text="NAME"
		    android:textStyle="bold"	
		    android:textColor="@color/white"
		    android:layout_weight="2.5"
		    android:layout_marginLeft="4dp"			   
	   	/>				
	   	
		<TextView		
		    android:id="@+id/result_start_number_title" 	
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:textSize="16dp"		    
		    android:text="#"
		    android:textStyle="bold"	
		    android:textColor="@color/white"
		    android:layout_weight="0.5"		
		    android:gravity="center_horizontal" 	   
	   	/>					
		
		<TextView		 	
		    android:id="@+id/result_team_title"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:textSize="16dp"		    
		    android:text="TEAM"
		    android:textStyle="bold"	
		    android:textColor="@color/white"
		    android:layout_weight="2"				   
	   	/>					
	   	
		<TextView		 	
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:textSize="16dp"		    
		    android:text="TIME"
		    android:textStyle="bold"
		    android:textColor="@color/white"
		    android:layout_weight="1"	
		    android:gravity="center_horizontal" 
	   	/>	
	   	
		<TextView		 	
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:textSize="16dp"		    
		    android:text="TIME BACK"
		    android:textStyle="bold"	
		    android:textColor="@color/white"
		    android:layout_weight="1"	
		    android:gravity="center_horizontal" 
	   	/>					
	</LinearLayout>
    	   					    
    <LinearLayout 
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:layout_marginLeft="8dp"
		android:layout_marginRight="8dp"
		>
        	
		<TextView		 	
		    android:id="@+id/result_name"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:textSize="16dp"		    
		    android:text="1"
		    android:layout_weight="2.5"
		    android:textStyle="bold"
	   	/>				   				

		<TextView		 	
		    android:id="@+id/result_start_number"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:textSize="16dp"		    
		    android:text="1"
		    android:layout_weight="0.5"
		    android:textStyle="bold"
		    android:gravity="center_horizontal" 
	   	/>					
		
		<TextView		 	
		    android:id="@+id/result_team"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:textSize="16dp"		    
		    android:text="1"
		    android:layout_weight="2"
		    android:textStyle="bold"
	   	/>						
		
		<TextView		 	
		    android:id="@+id/result_time"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:textSize="16dp"		    
		    android:text="1"
		    android:layout_weight="1"
		 	android:gravity="center_horizontal" 
	   	/>				   				
	   	
		<TextView		 	
		    android:id="@+id/result_time_back"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:textSize="16dp"		    
		    android:text="1"
		    android:layout_weight="1"
		    android:gravity="center_horizontal" 	    
	   	/>			            	            
	</LinearLayout>       
</LinearLayout>	